<?xml version="1.0" encoding="utf-8"?>
<cn.lovepet.shops.view.widget.percentlayout.PercentFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <cn.lovepet.shops.view.ui.activity.content.albumhomegoods.BGABanner
        android:id="@+id/middle_banner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:banner_indicatorGravity="bottom"
        app:banner_pageChangeDuration="1200"
        app:banner_placeholderDrawable="@drawable/bk_defalt"
        app:banner_pointContainerBackground="@color/transparent"
        app:banner_tipTextSize="12sp"
        app:banner_transitionEffect="defaultEffect"
        app:banner_viewAspectRatio="4.24"
        app:layout_marginLeftPercentScreenX="2%"
        app:layout_marginRightPercentScreenX="2%"
        app:layout_marginTopPercentScreenX="2%" />
</cn.lovepet.shops.view.widget.percentlayout.PercentFrameLayout>
